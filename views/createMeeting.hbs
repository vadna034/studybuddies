<div class="dash">
    <div class="addForm">
        <form name="createMeeting" action="/createMeeting" method="POST">

            <div class="form-group row">
                <label for="Date" class="col-sm-2 col-form-label">
                    Date:
                </label>
                <div class="col-sm-5">
                    <input type="date" name="startDate">
                </div>
            </div>


            <div class="form-group row">
                <label for="startTime" class="col-sm-2 col-form-label">
                    Start Time:
                </label>
                <div class="col-sm-5">
                    <input type="time" name="startTime">
                </div>
            </div>


            <div class="form-group row">
                <label for="endTime" class="col-sm-2 col-form-label">
                    End Time:
                </label>
                <div class="col-sm-5">
                    <input type="time" name="endTime">
                </div>
            </div>

            <div class="form-group row">
                <label for="ZoomLink" class="col-sm-2 col-form-label">
                    Zoom Link:
                </label>
                <div class="col-sm-5">
                    <input type="text" name="ZoomLink">
                </div>
            </div>


            <div class="form-group row">
                <label for="purpose" class="col-sm-2 col-form-label">
                    Purpose (Optional):
                </label>
                <div class="col-sm-5">
                    <input type="text" name="purpose">
                </div>
            </div>


            <button type="submit" name="classId" value='{{classId}}' class="btn btn-primary">Submit</button>
            <div class="form-group">
            </div>
        </form>
    </div>

    <script> console.log("This works");</script>
</div>